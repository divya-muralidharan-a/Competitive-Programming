#include <bits/stdc++.h>
using namespace std;

int main() {
	string a;
	cin>>a;
	int c=0;
	string p="Danil",q="Olya",r="Slava",s="Ann",t="Nikita";
	if(a.find(p)!=string::npos)
	{
	    c++;
	    int found=a.find(p);
	    string k=a.substr(found+p.length(),a.length());
	    if(k.find(p)!=string::npos)
	    c++;
	}
	if(a.find(q)!=string::npos)
	{
	    c++;
	    int found=a.find(p);
	    string k=a.substr(found+p.length(),a.length());
	    if(k.find(p)!=string::npos)
	    c++;
	}
	if(a.find(r)!=string::npos)
	{
	    c++;
	    int found=a.find(r);
	    string k=a.substr(found+r.length(),a.length());
	    if(k.find(r)!=string::npos)
	    c++;
	}
	if(a.find(s)!=string::npos)
	{
	    c++;
	    int found=a.find(s);
	    string k=a.substr(found+s.length(),a.length());
	    if(k.find(s)!=string::npos)
	    c++;
	}
	if(a.find(t)!=string::npos)
	{
	    c++;
	    int found=a.find(t);
	    string k=a.substr(found+t.length(),a.length());
	    if(k.find(t)!=string::npos)
	    c++;
	}
	if(c!=1)
	cout<<"No"<<endl;
	else
	cout<<"Yes"<<endl;
	return 0;
}
